package com.joy.SpringFramework_1;

public class Tyre {
	
	@Override
	public String toString() {
		return "Tyre [brand=" + brand + "]";
	}

	private String brand;
	
	public Tyre() {}

	public Tyre(String brand) {
		super();
		this.brand = brand;
	}

	public String getBrand() {
		return brand;
	}

	public void setBrand(String brand) {
		this.brand = brand;
	}
}
